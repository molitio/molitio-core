{
    "name": "@molitio/ui-core",
    "version": "0.12.5-alpha.0",
    "author": "molitio foundation",
    "description": "Universal reusable React UI Components",
    "license": "CC0-1.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/molitio/molitio-core.git"
    },
    "keywords": [
        "molitio",
        "ui-core"
    ],
    "bugs": {
        "url": "https://github.com/molitio/molitio-core/issues"
    },
    "homepage": "https://github.com/molitio/molitio-core#readme",
    "main": "dist/index.js",
    "module": "dist/esm/index.js",
    "typings": "dist/index.d.ts",
    "outputTestSample": "tests/index.js",
    "bundle": {
        "esm": "dist/bundle/esm/index.js",
        "umd": "dist/bundle/umd/index.js"
    },
    "iife": {
        "radioPage": "dist/iife/molitio-radio-page.js"
    },
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=12"
    },
    "husky": {
        "hooks": {
            "pre-commit": "eslint '*/**/*.{js,ts,tsx, css, scss}' --quiet --fix"
        }
    },
    "size-limit": [
        {
            "path": "dist/index.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/esm/index.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/bundle/esm/index.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/bundle/umd/index.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/types/index.d.ts",
            "limit": "10 KB"
        }
    ],
    "resolutions": {
        "styled-components": "^5"
    },
    "scripts": {
        "prebuild": "npm run clean && prettier --write src/",
        "build": "rollup --config",
        "postbuild": "node packageBuilder && npm run create-artifacts",
        "build:watch": "rollup --config --watch",
        "prepublish": "npm run build",
        "publish": "npm publish ./dist",
        "lint": "eslint '*/**/*.{js,ts,tsx, css, scss}'",
        "lint-fix": "eslint '*/**/*.{js,ts,tsx, css, scss}' --quiet --fix",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "storybook": "cross-env STORYBOOK_DISPLAY_WARNING=true DISPLAY_WARNING=true start-storybook -p 4242 --no-manager-cache",
        "build-storybook": "cd .artifact && rm -rf storybook && mkdir storybook && cd .. && cross-env STORYBOOK_DISPLAY_WARNING=true DISPLAY_WARNING=true build-storybook -o .artifact/storybook --docs",
        "debug": "cross-env NODE_OPTIONS=--inspect-brk STORYBOOK_DISPLAY_WARNING=true DISPLAY_WARNING=true start-storybook -p 4242",
        "create-artifacts": "mkdir -p .artifact/package && cd dist/ && npm pack --pack-destination ../.artifact/package && cd .. && npm run docgen",
        "clean": "rm -rf dist && rm -rf .artifact",
        "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
        "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --watch",
        "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
        "docgen": "mkdir -p .artifact/jsdoc && ./node_modules/.bin/jsdoc src/index.ts -d .artifact/jsdoc",
        "tsgen": "tsc --project tsconfig.json",
        "testgen": "npm run tsc __tests__ -d .artifact/__tests__ --project tsconfig.json"
    },
    "peerDependencies": {
        "react": "^18.1.0",
        "react-dom": "^18.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.0",
        "@babel/plugin-transform-typescript": "^7.16.1",
        "@babel/preset-env": "^7.18.2",
        "@babel/preset-react": "^7.17.12",
        "@babel/preset-typescript": "^7.16.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@fortawesome/react-fontawesome": "^0.1.16",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-buble": "^0.21.3",
        "@rollup/plugin-commonjs": "^22.0.0",
        "@rollup/plugin-image": "^2.1.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.0.6",
        "@rollup/plugin-replace": "^3.0.0",
        "@rollup/plugin-typescript": "^8.3.0",
        "@storybook/addon-a11y": "^6.5.4",
        "@storybook/addon-actions": "^6.5.4",
        "@storybook/addon-essentials": "^6.5.4",
        "@storybook/addon-interactions": "^6.5.4",
        "@storybook/addon-jest": "^6.5.6",
        "@storybook/addon-links": "^6.5.4",
        "@storybook/addons": "^6.5.4",
        "@storybook/builder-webpack5": "^6.5.4",
        "@storybook/manager-webpack5": "^6.5.4",
        "@storybook/preset-scss": "^1.0.3",
        "@storybook/preset-typescript": "^3.0.0",
        "@storybook/react": "^6.5.4",
        "@storybook/react-docgen-typescript-plugin": "^1.0.2-canary.6.9d540b91e815f8fc2f8829189deb00553559ff63.0",
        "@storybook/testing-library": "^0.0.11",
        "@storybook/theming": "^6.5.4",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.11.10",
        "@types/react": "^18.0.9",
        "@types/react-dom": "^18.0.5",
        "@types/rollup-plugin-peer-deps-external": "^2.2.1",
        "@types/styled-components": "^5.1.25",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "babel-cli": "^6.26.0",
        "babel-jest": "^28.1.0",
        "babel-loader": "^8.2.3",
        "babel-preset-latest": "^6.24.1",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "device-detector-js": "^3.0.2",
        "eslint": "^8.3.0",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-storybook": "^0.5.5",
        "fork-ts-checker-webpack-plugin": "^6.5.0",
        "husky": "^7.0.4",
        "jest": "^28.1.0",
        "jest-config": "^28.1.0",
        "jest-environment-jsdom": "^28.1.0",
        "jsdoc": "~3.6.10",
        "node-sass": "^7.0.1",
        "npm": "^8.7.0",
        "postcss": "^8.4.14",
        "postcss-scss": "^4.0.4",
        "prettier": "2.6.2",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-jss": "^10.9.0",
        "rollup": "^2.75.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-size": "^0.2.2",
        "sass": "^1.52.1",
        "sass-loader": "^12.6.0",
        "size-limit": "^7.0.3",
        "style-loader": "^3.3.1",
        "styled-components": "^5.3.5",
        "ts-jest": "^28.0.4",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.8.0",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "tslib": "^2.3.1",
        "typescript": "^4.7.3",
        "typescript-plugin-css-modules": "^3.4.0"
    },
    "workspaces": [
        "src",
        ".storybook"
    ],
    "dependencies": {
        "nanoid": "^4.0.0"
    }
}
